# Server Configuration
PORT=5000
NODE_ENV=production

# MongoDB Connection
MONGO_URI=mongodb+srv://your_atlas_username:your_password@your_cluster.mongodb.net/cloudshare?retryWrites=true&w=majority

# Clerk Authentication
